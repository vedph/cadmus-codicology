<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net7.0</TargetFramework>
    <IncludeSymbols>true</IncludeSymbols>
	<Nullable>enable</Nullable>
	<SymbolPackageFormat>snupkg</SymbolPackageFormat>
	<Version>5.0.6</Version>
	<Authors>Daniele Fusi</Authors>
	<Company>Fusi</Company>
	<Product>Cadmus</Product>
	<Description>Codicological parts for Cadmus.</Description>
	<Copyright>by Daniele Fusi 2022</Copyright>
	<NeutralLanguage>en-US</NeutralLanguage>
	<PackageLicenseExpression>GPL-3.0-or-later</PackageLicenseExpression>
	<PackageTags>Cadmus;parts</PackageTags>
	<FileVersion>5.0.6</FileVersion>
	<AssemblyVersion>5.0.6</AssemblyVersion>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Cadmus.Core" Version="6.1.4" />
    <PackageReference Include="Cadmus.Mat.Bricks" Version="4.0.2" />
    <PackageReference Include="Cadmus.Refs.Bricks" Version="4.0.1" />
    <PackageReference Include="Fusi.Antiquity" Version="3.0.0" />
  </ItemGroup>

</Project>
